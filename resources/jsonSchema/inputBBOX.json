{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://json-schema.org/jsonld/inputBBOX,json#",
    "title": "Input BBOX",
    "description": "Schema of input for BBOX operation",
    "type": "object",
    "properties": {
        "features": { 
			"oneOf": [
				{"$ref": "http://json-schema.org/geojsonld/geojsonld.json#" },
				{"type": "string",
				 	"format": "uri" }
			]
		}
    },
    "required": [ "features"]
}